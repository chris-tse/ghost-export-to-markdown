#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ctse/code/ghost-export-to-markdown/bin/node_modules:/Users/ctse/code/ghost-export-to-markdown/node_modules:/Users/ctse/code/node_modules:/Users/ctse/node_modules:/Users/node_modules:/node_modules:/Users/ctse/code/ghost-export-to-markdown/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/ctse/code/ghost-export-to-markdown/bin/node_modules:/Users/ctse/code/ghost-export-to-markdown/node_modules:/Users/ctse/code/node_modules:/Users/ctse/node_modules:/Users/node_modules:/node_modules:/Users/ctse/code/ghost-export-to-markdown/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../ghost-export-to-markdown/bin/cli"   "$@"
exit $?
